      "displayName": "Customer & Sales Relationship",
      "queryLines": [
        "SELECT c.*, s.* FROM business.coffee_shops.dim_customers_analytics AS c\r\n",
        "INNER JOIN business.coffee_shops.fact_sales_transactions_analytics AS s\r\n",
        "ON c.customerID=s.customerID;"
      ]
    },
    {
      "name": "a881730f",
      "displayName": "Franchises & Suppliers Relationship",
      "queryLines": [
        "SELECT f.*, su.* FROM business.coffee_shops.dim_franchises_analytics AS f\r\n",
        "INNER JOIN business.coffee_shops.dim_suppliers_analytics AS su\r\n",
        "ON f.supplierID=su.supplierID;"
      ]
    },
